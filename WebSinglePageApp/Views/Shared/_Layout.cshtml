@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>
@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<List<WebSinglePageApp.Models.Title>>




<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="stylesheet" href="~/Contents/css/css1.css">
    <link rel="stylesheet" href="~/Contents/css/HeaderStyleSheet1.css">
    <link rel="stylesheet" href="~/Contents/css/mui.css">
    <link rel="stylesheet" href="~/Contents/css/StyleSheet.scss">

    <link href='http://fonts.googleapis.com/css?family=Cookie' rel='stylesheet' type='text/css'>

    @RenderSection("scriptsHead", required: false)


    

</head>
<body >
    
    
        <header class="header-basic">
            <div class="header-limiter">
                @{
                    foreach (var name in Model)
                    {
                        if (name.Name == "Title")
                        {
                            <h1><a href="home" style="font-family:Tahoma"> PAGE TITLE :<Span style="padding-left:16px">This Page is A single Page Application </Span></a></h1>
                        }
                        else
                        {
                            <h1><a href="home" style="font-family:Tahoma"> PAGE TITLE :<Span style="padding-left:16px"> @name.Name</Span></a></h1>

                        }

                    }
                }


            </div>
        </header>


        <ul id="categories"></ul>

    <div class="contentContainer" id="main">
        @RenderSection("scriptPageContent", required: false)
        @RenderBody()
    </div>




        <script src="~/Scripts/jquery.min.js" type="text/javascript"></script>
        <script src="~/Scripts/js/mui.min.js" type="text/javascript"></script>

        @RenderSection("scriptsBottom", required: false)

    

</body>

</html>

